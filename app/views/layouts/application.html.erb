<!DOCTYPE html>
<html>
<head>
  <title>Cjproject</title>
  <%= include_gon %>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
  <div class="container">
    <div class="row">
      <%= yield :home %>
    </div> 
    <div class="row">
      <div class="span1"></div>
      <div class="span3">
        <%= yield :table%>
      </div>
      <div class="span6">
        <%= yield :diceboard %>
      </div>
      
      <div class="span2">
        <div id="user_nav">
	  <%= yield :userstat %>  
        </div>
      </div>
  
      </div>
  </div>
</body>
</html>
