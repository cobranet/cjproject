<% if @jamb.diceboard.mode != :end %> 
  <%= render partial: "roll_button" %> 
  <% if @jamb.diceboard.mode.to_sym != :no_roll %>
    <%= render partial: "roll_other" %>
  <%end%>
<%else%>
  <%= link_to "#",  { :method=>:post, :class =>  "btn btn-primary btn-large",:remote => true } do %>
